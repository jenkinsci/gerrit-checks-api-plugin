[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/Thomas Dräbing (tdraebing)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Thomas Dräbing (tdraebing)
	label-Verified = -1..+1 group user/Thomas Dräbing (tdraebing)
	push = group user/Thomas Dräbing (tdraebing)
	read = group user/Thomas Dräbing (tdraebing)
	rebase = group user/Thomas Dräbing (tdraebing)
	removeReviewer = group user/Thomas Dräbing (tdraebing)
	submit = group user/Thomas Dräbing (tdraebing)
[access "refs/heads/*"]
	create = group user/Thomas Dräbing (tdraebing)
	push = +force group user/Thomas Dräbing (tdraebing)
	pushMerge = group user/Thomas Dräbing (tdraebing)
	read = group user/Thomas Dräbing (tdraebing)
[access "refs/tags/*"]
	push = group user/Thomas Dräbing (tdraebing)
	push = +force group user/Thomas Dräbing (tdraebing)
